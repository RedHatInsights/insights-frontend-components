{
  "name": "@red-hat-insights/insights-frontend-components",
  "version": "3.34.1",
  "main": "dist/index.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "index.css",
    "components/",
    "Utilities/"
  ],
  "dependencies": {
    "@patternfly/react-core": "^1.51.4",
    "@patternfly/react-icons": "^2.8.0",
    "@patternfly/react-table": "^0.4.18",
    "abortcontroller-polyfill": "^1.2.1",
    "apollo-boost": "^0.1.23",
    "bootstrap-sass": "^3.3.7",
    "c3": "^0.6.7",
    "classnames": "^2.2.5",
    "d3": "^5.7.0",
    "font-awesome-sass": "^4.7.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "marked": "^0.6.0",
    "patternfly-react": "^2.19.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.5.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^5.1.1",
    "urijs": "^1.19.1"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js|jsx}",
      "!src/**/stories/*",
      "!src/**/index.js",
      "!src/**/*{c|C}ontext*.js"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "sassIncludes": {
    "patternfly": "node_modules/patternfly/dist/sass",
    "bootstrap": "node_modules/bootstrap-sass/assets/stylesheets",
    "fontAwesome": "node_modules/font-awesome-sass/assets/stylesheets"
  },
  "devDependencies": {
    "@khala/commit-analyzer-wildcard": "^1.0.2",
    "@patternfly/patternfly-next": "^1.0.175",
    "@semantic-release/git": "^6.0.2",
    "@semantic-release/npm": "^3.4.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^ 9.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-dual-import": "^1.2.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "codecov": "^3.1.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-fetch": "^2.2.0",
    "css-loader": "^0.28.11",
    "dot": "^1.1.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^1.1.11",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^4.0.0-alpha.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.4.2",
    "moment": "^2.22.1",
    "node-sass": "4.9.3",
    "react-content-loader": "^3.4.1",
    "redux-mock-store": "^1.5.3",
    "sanitize-html": "^1.20.0",
    "sass-loader": "^7.1.0",
    "semantic-release": "^15.9.15",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.21.0",
    "topojson": "^3.0.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-visualizer-plugin": "^0.1.11",
    "write-file-webpack-plugin": "^4.4.0"
  },
  "resolutions": {
    "node-sass": "4.9.3"
  },
  "scripts": {
    "start": "npm run build",
    "build": "eslint config src && webpack --config config/webpack-build.config.js",
    "test": "jest",
    "coverage": "codecov",
    "prepublish": "rm -rf ./dist && npm run build",
    "playground": "webpack-dev-server --config config/webpack-build.config.js --env.server=true",
    "release": "semantic-release",
    "lint": "eslint config src",
    "watch": "webpack --watch --config config/webpack-build.config.js"
  },
  "release": {
    "analyzeCommits": "@khala/commit-analyzer-wildcard/analyzer",
    "prepare": [
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json"
        ],
        "message": "Release of new version: ${nextRelease.version} <no> [skip ci]"
      }
    ]
  },
  "description": "Components for Red Hat Insights Applications",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RedHatInsights/insights-frontend-components.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
